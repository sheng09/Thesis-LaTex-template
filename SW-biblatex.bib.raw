Publisher: Nature Publishing Group},
	file = {Steinle-Neumann et al_2001_Elasticity of iron at the temperature of the Earth's inner core.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2001/Steinle-Neumann et al_2001_Elasticity of iron at the temperature of the Earth's inner core.pdf:application/pdf;Snapshot:/Users/abc123/Zotero/storage/WSYGBXS6/35092536.html:text/html}
