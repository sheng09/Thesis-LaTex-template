	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/grl.50237},
	keywords = {Body waves, Seismic interferometry, Core phases, cross-correlation},
	file = {Lin et al_2013_Extracting seismic core phases with array interferometry.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2013/Lin et al_2013_Extracting seismic core phases with array interferometry.pdf:application/pdf}
}

@article{lin_seismic_2013,
