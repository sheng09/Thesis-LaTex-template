	volume = {95},
	rights = {Copyright 1990 by the American Geophysical Union.},
	issn = {2156-2202},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/JB095iB13p21691},
	doi = {10.1029/JB095iB13p21691},
	abstract = {We review the current state of the seismological constraints on core structure. The evidence supports a simple two-layer model of the core. The fluid outer core seems to have properties which are consistent with those of a vigorously convecting region. The inner core has a sharp boundary characterised by a 0.5–0.6 g/cc density jump, a 0.6–0.7 km/s jump in P velocity, and a jump in shear velocity to 2.5 km/s or greater. There is weak evidence for an anomalously steep shear velocity gradient at the top of the inner core. Finally, the attenuation of both body waves and free oscillations indicate that the inner core is strongly attenuating near its surface though the data suggest that the attenuation is frequency dependent},
	pages = {21691--21695},
	issue = {B13},
	journaltitle = {Journal of Geophysical Research: Solid Earth},
	author = {Masters, T. G. and Shearer, P. M.},
	urldate = {2021-06-15},
	date = {1990},
	langid = {english},
	note = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/{JB}095iB13p21691},
	file = {Masters_Shearer_1990_Summary of seismological constraints on the structure of the Earth's core.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/1990/Masters_Shearer_1990_Summary of seismological constraints on the structure of the Earth's core.pdf:application/pdf}
}

@article{okal_detection_1998,
	title = {Detection of {PKJKP} at intermediate periods by progressive multi-channel correlation},
	volume = {164},
	issn = {0012-821X},
	url = {https://www.sciencedirect.com/science/article/pii/S0012821X98002106},
	doi = {10.1016/S0012-821X(98)00210-6},
	abstract = {We have identified the seismic phase {PKJKP}, traveling through the inner core of the Earth as a shear wave, in intermediate-period (2–10 s) records of the deep 1996 Flores Sea earthquake at eight stations of the French seismic network. This constitutes direct evidence of the solidity of the inner core which, while generally recognized, was until now only inferred from indirect evidence. The arrival times on stacked seismograms require a shear-wave velocity at the top of the inner core β{ICB}=3.65 km/s in agreement with values suggested from normal mode observations. Julian et al.'s 1972 observation of a lower velocity (2.95 km/s) is easily reconciled with our result if interpreted as the surface reflection {pPKJKP}. The high Poisson ratio (ν=0.44) can be reconciled with a normal crystalline structure without invoking partial melting on account of the overwhelming pressure at the center of the Earth.},
	pages = {23--30},
	number = {1},
	journaltitle = {Earth and Planetary Science Letters},
	shortjournal = {Earth and Planetary Science Letters},
	author = {Okal, Emile A. and Cansi, Yves},
	urldate = {2021-06-15},
	date = {1998-12-15},
	langid = {english},
	keywords = {inner core, earthquakes, {PKP}-waves, S-waves},
	file = {Okal_Cansi_1998_Detection of PKJKP at intermediate periods by progressive multi-channel.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/1998/Okal_Cansi_1998_Detection of PKJKP at intermediate periods by progressive multi-channel.pdf:application/pdf}
}

@article{antonangeli_elastic_2004,
	title = {Elastic anisotropy in textured hcp-iron to 112 {GPa} from sound wave propagation measurements},
	volume = {225},
	issn = {0012-821X},
	url = {https://www.sciencedirect.com/science/article/pii/S0012821X04003826},
	doi = {10.1016/j.epsl.2004.06.004},
	abstract = {We report the first experimental determination of elastic anisotropy in highly textured polycrystalline hexagonal-closed-packed (hcp)-iron obtained at room temperature and 112 {GPa} by inelastic X-ray scattering ({IXS}) in a diamond anvil cell ({DAC}). The compressional sound velocity {VP} at 50° and 90° from the c-axis was determined to be {VP}\{50°\}=9900±200 m/s and {VP}\{90°\}=9450±150 m/s, respectively. The difference of 4–5\% between the two velocities is of the same order of the seismic observations of the anisotropy in Earth's inner core. Combining our new results with the equation of state of hcp-iron, we derive the pressure dependence of the shear velocity {VS}. The {VS}-values, extrapolated to inner core densities, are 30\% higher than the {PREM} results. The extrapolated {VP}-values are instead in fair agreement with the {PREM} seismic model, though differences in the slope remain. Furthermore, assuming a strong texture characterized by cylindrical symmetry, we provide an estimate for the single crystal elastic modulus C11 as a function of pressure. While our results are in general agreement with the predominant existence of hcp-iron in Earth's inner core, the remaining differences might be assigned to the presence of another solid iron phase, light elements, or liquid inclusions as well as anharmonic high-temperature effects.},
	pages = {243--251},
	number = {1},
	journaltitle = {Earth and Planetary Science Letters},
	shortjournal = {Earth and Planetary Science Letters},
	author = {Antonangeli, Daniele and Occelli, Florent and Requardt, Herwig and Badro, James and Fiquet, Guillaume and Krisch, Michael},
	urldate = {2021-06-16},
	date = {2004-08-30},
	langid = {english},
	keywords = {Earth's inner core, elastic anisotropy, high pressure, hcp-iron, inelastic x-ray scattering, sound velocities},
	file = {Antonangeli et al_2004_Elastic anisotropy in textured hcp-iron to 112 GPa from sound wave propagation.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2004/Antonangeli et al_2004_Elastic anisotropy in textured hcp-iron to 112 GPa from sound wave propagation.pdf:application/pdf}
}

@article{mao_elasticity_1998,
