@incollection{souriau_123_2015,
	location = {Oxford},
	title = {1.23 - Deep Earth Structure: The Earth’s Cores},
	isbn = {978-0-444-53803-1},
	url = {https://www.sciencedirect.com/science/article/pii/B9780444538024000208},
	shorttitle = {1.23 - Deep Earth Structure},
	abstract = {The Earth's core is composed of a fluid outer core, with a radius of 3480km, and a solid inner core, with a radius of about 1220km, which results from the freezing of the liquid core during the cooling of the Earth. Travel times, amplitudes, and waveforms of waves propagating through the core and normal modes having energy deep inside the Earth are the most powerful tools for investigating core structure. Important constraints are also provided by Earth dynamics, high-pressure physics, magnetic field analyses, and geochemical data. From seismic wave propagation, the liquid core appears homogeneous and nonstratified, except close to the inner core boundary ({ICB}) and perhaps close to the core–mantle boundary ({CMB}). The smooth P-velocity and density increase with depth reflects the pressure and temperature increase. The liquid core density, lower than the density of pure iron, indicates the presence of light elements in the liquid iron alloy, in an amount of about 10\%. The liquid core has a very high-quality factor (almost no wave attenuation). Its surface, the {CMB}, is a first-order discontinuity. Its topography is difficult to map because of the perturbing effect of the strong heterogeneities at the base of the mantle, but it may be small, {\textless}2–3km at long wavelengths ({\textasciitilde}1000km). Small topography ({\textasciitilde}100m) of kilometric wavelength responsible for wave scattering may also be present. The lowermost 150km at the base of the liquid core corresponds to a low P-velocity gradient, likely related to a density increase. This dense layer may possibly result from partial remelting of the frozen iron alloy in relation to inner core and outer core and outer core dynamics. The {ICB} appears as a first-order discontinuity. It corresponds to a small P-velocity and density increase with respect to the liquid core, which indicates an average depletion of the inner core in light elements, compared to the liquid core. No significant topography at the {ICB} could be safely detected to date, except perhaps at short wavelength from scattered waves. The rigidity of the inner core is well established from normal modes. Body-wave analyses favor an increase in S-velocities with depth, with low values at the {ICB}, possibly due to the presence of fluid inclusions in the solid iron alloy matrix. The seismic attenuation in the inner core is globally strong; the relative contributions of scattering and viscoelasticity are still debated. The inner core has a complex structure that is still debated and whose image evolves as new data become available. An important property of the inner core is its anisotropy, evidenced from both P-wave propagation and normal modes. P-wave velocity is 1–3\% higher in the direction parallel to the Earth's rotation axis than in the direction parallel to the equatorial plane. Oriented anisotropic iron crystals may explain this anisotropy, but the mechanism at the origin of crystal alignment is still debated. Depth variations in anisotropy are present: the top 100–150km is nearly isotropic, and the lowermost 300–600km (the innermost inner core) shows a different anisotropy, which is still to be specified. Some anisotropy in attenuation is also observed, with high attenuation in the direction of high velocity. The most intriguing feature of the inner core is the presence of a hemispherical pattern, observed both in the properties of the isotropic layer and in the degree of anisotropy below this layer: the uppermost 80km of the inner core has a higher mean velocity and a stronger attenuation in the eastern hemisphere than in the western hemisphere, defined with respect to the origin meridian. On the other hand, the western hemisphere is more anisotropic in both velocity and attenuation than the eastern hemisphere. This difference in apparent anisotropy may denote a variation in the degree of crystal alignment or a thicker isotropic layer beneath the eastern hemisphere. Wave scattering on iron grains with various sizes and orientations may explain both the observed variations in velocity and attenuation and their correlation, opposite to the one observed in the mantle. A possible faster rotation of the inner core with respect to the mantle is predicted by some geodynamo models, but thwarted by the gravimetric coupling with the mantle, which prevents a large steady rotation. Attempts to detect it rely on the analysis of a long series of seismic data sampling either specific paths or the whole Earth, from body waves or normal modes. The inferred rotation rates are very scattered, with most of the values below 0.2° year−1. A null rotation or small oscillations about the rotation axis are favored by the most recent results. Numerous open questions remain, concerning the structure at the core boundaries and the stratification at the base and top of the liquid core, the hemisphericity of the inner core, the structure of the innermost inner core, and the nature and texture of iron alloy. All these questions have strong implications on core dynamics and Earth differentiation. Future core exploration will, however, depend on the quality of Earth sampling by seismic data, thus on the deployment of observatories in poorly sampled continental areas and in oceans.},
	pages = {725--757},
	booktitle = {Treatise on Geophysics (Second Edition)},
	publisher = {Elsevier},
	author = {Souriau, A. and Calvet, M.},
	editor = {Schubert, Gerald},
	urldate = {2021-07-13},
	date = {2015-01-01},
	langid = {english},
	doi = {10.1016/B978-0-444-53802-4.00020-8},
	keywords = {Attenuation, Anisotropy, Seismology, Earth's core, Inner core rotation, Seismic velocities}
}

@article{tkalcic_large_2010,
	title = {Large variations in travel times of mantle-sensitive seismic waves from the South Sandwich Islands: Is the Earth's inner core a conglomerate of anisotropic domains?},
	volume = {37},
	issn = {1944-8007},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2010GL043841},
	doi = {10.1029/2010GL043841},
	shorttitle = {Large variations in travel times of mantle-sensitive seismic waves from the South Sandwich Islands},
	abstract = {Cylindrical anisotropy in Earth's inner core has been invoked to account for travel times of {PKP} core-sensitive seismic waves, such as from the South Sandwich Islands ({SSI}) earthquakes observed in Alaska, which depart from predictions. Newly collected travel-time residuals from seismic waves from the {SSI} region that sample only Earth's mantle ({PcP} and P waves) have a comparable range to the {PKP} differential travel-time residuals, yet they are insensitive to core structure. This observation suggests that mantle structure affects {PKP} travel time residuals more than previously acknowledged and challenges the existing conceptual framework of a uniform inner core anisotropy. The inner core could be a conglomerate of anisotropic domains, and the {PKP} travel times are most likely influenced by the geometry of inner core sampling and inhomogeneous mantle structure. This concept reconciles observed complexities in travel times while preserving a net inner core anisotropy that is required by observations of Earth's free oscillations.},
	number = {14},
	journaltitle = {Geophysical Research Letters},
	author = {Tkalčić, Hrvoje},
	urldate = {2021-07-13},
	date = {2010},
	langid = {english},
	note = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2010GL043841},
	keywords = {body waves, Earth's core, elastic anistropy},
	file = {Full Text PDF:/Users/abc123/Zotero/storage/BQXB25SC/Tkalčić - 2010 - Large variations in travel times of mantle-sensiti.pdf:application/pdf}
}

