@article{sun_repet_2022,
	title = {Repetitive marsquakes in Martian upper mantle},
	volume = {13},
	rights = {2022 The Author(s)},
	issn = {2041-1723},
	url = {https://www.nature.com/articles/s41467-022-29329-x},
	doi = {10.1038/s41467-022-29329-x},
	abstract = {Marsquakes excite seismic wavefield, allowing the Martian interior structures to be probed. However, the Martian seismic data recorded by {InSight} have a low signal-to-noise ratio, making the identification of marsquakes challenging. Here we use the Matched Filter technique and Benford’s Law to detect hitherto undetected events. Based on nine marsquake templates, we report 47 newly detected events, {\textgreater}90\% of which are associated with the two high-quality events located beneath Cerberus Fossae. They occurred at all times of the Martian day, thus excluding the tidal modulation (e.g., Phobos) as their cause. We attribute the newly discovered, low-frequency, repetitive events to magma movement associated with volcanic activity in the upper mantle beneath Cerberus Fossae. The continuous seismicity suggests that Cerberus Fossae is seismically highly active and that the Martian mantle is mobile.},
	pages = {1695},
	number = {1},
	journaltitle = {Nature Communications},
	shortjournal = {Nat Commun},
	author = {Sun, Weijia and Tkalčić, Hrvoje},
	urldate = {2022-04-06},
	date = {2022-03-30},
	langid = {english},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Seismology, Geodynamics},
	file = {41467_2022_29329_MOESM1_ESM.pdf:/Users/abc123/Zotero/storage/65HU3I2I/41467_2022_29329_MOESM1_ESM.pdf:application/pdf;41467_2022_29329_MOESM2_ESM.pdf:/Users/abc123/Zotero/storage/LFSHGKA3/41467_2022_29329_MOESM2_ESM.pdf:application/pdf;Sun_Tkalčić_2022_Repetitive marsquakes in Martian upper mantle.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2022/Sun_Tkalčić_2022_Repetitive marsquakes in Martian upper mantle.pdf:application/pdf}
