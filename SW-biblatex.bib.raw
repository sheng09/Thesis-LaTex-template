@article{deguen_structure_2012,
	title = {Structure and dynamics of Earth's inner core},
	volume = {333-334},
	issn = {0012-821X},
	url = {http://www.sciencedirect.com/science/article/pii/S0012821X12002051},
	doi = {10.1016/j.epsl.2012.04.038},
	abstract = {The seismological exploration of the Earth's inner core has revealed unexpected and puzzling structural complexities. Its elastic anisotropy is now well established, and has been shown to vary spatially. There is a well defined hemispherical dichotomy in anisotropy and attenuation, and significant variations in the radial direction, with weaker anisotropy in the uppermost regions, and possibly different properties in an “innermost inner core” of radius 300–600km. Perhaps even more puzzling is the observation of a stably stratified layer at the base of the outer core, which has recently been proposed to result from melting of inner core material. If confirmed, the presence of this layer has major implications for our understanding of convection in the outer core. Inner core differential rotation with respect to the mantle remains controversial. We will review here recent progress in understanding the dynamics of the inner core. A number of different models have been proposed for the development of the inner core's structure and formation of the stable layer at the base of the outer core, and particular care will be taken to clarify under which conditions the proposed mechanisms can be active.},
	pages = {211--225},
	journaltitle = {Earth and Planetary Science Letters},
	shortjournal = {Earth and Planetary Science Letters},
	author = {Deguen, Renaud},
	urldate = {2020-12-22},
	date = {2012-06-01},
	langid = {english},
	keywords = {inner core, anisotropy, deformation texturing, differential rotation, F-layer, solidification texturing},
	file = {Deguen_2012_Structure and dynamics of Earth's inner core.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2012/Deguen_2012_Structure and dynamics of Earth's inner core.pdf:application/pdf;ScienceDirect Snapshot:/Users/abc123/Zotero/storage/T2JCY2EF/S0012821X12002051.html:text/html}
}

@article{deuss_heterogeneity_2014,
	title = {Heterogeneity and Anisotropy of Earth's Inner Core},
	volume = {42},
	url = {https://doi.org/10.1146/annurev-earth-060313-054658},
	doi = {10.1146/annurev-earth-060313-054658},
	abstract = {Seismic observations provide strong evidence that Earth's inner core is anisotropic, with larger velocity in the polar than in the equatorial direction. The top 60–80 km of the inner core is isotropic; evidence for an innermost inner core is less compelling. The anisotropy is most likely due to alignment of hcp (hexagonal close-packed) iron crystals, aligned either during solidification or by deformation afterward. The existence of hemispherical variations used to be controversial, but there is now strong evidence from both seismic body wave and normal mode observations, showing stronger anisotropy, less attenuation, and a lower isotropic velocity in the western hemisphere. Two mechanisms have been proposed to explain the hemispherical pattern: either (a) inner core translation, wherein one hemisphere is melting and the other is solidifying, or (b) thermochemical convection in the outer core, leading to different solidification conditions at the inner core boundary. Neither is (yet) able to explain all seismically observed features, and a combination of different mechanisms is probably required.},
	pages = {103--126},
	number = {1},
	journaltitle = {Annual Review of Earth and Planetary Sciences},
	author = {Deuss, Arwen},
	urldate = {2020-12-22},
	date = {2014},
	note = {\_eprint: https://doi.org/10.1146/annurev-earth-060313-054658},
	file = {Deuss_2014_Heterogeneity and Anisotropy of Earth's Inner Core.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2014/Deuss_2014_Heterogeneity and Anisotropy of Earth's Inner Core.pdf:application/pdf}
}

@article{mattesini_hemispherical_2010,
	title = {Hemispherical anisotropic patterns of the Earth’s inner core},
	volume = {107},
	issn = {0027-8424, 1091-6490},
	url = {https://www.pnas.org/content/107/21/9507},
	doi = {10.1073/pnas.1004856107},
	abstract = {It has been shown that the Earth’s inner core has an axisymmetric anisotropic structure with seismic waves traveling ∼3\% faster along polar paths than along equatorial directions. Hemispherical anisotropic patterns of the solid Earth’s core are rather complex, and the commonly used hexagonal-close-packed iron phase might be insufficient to account for seismological observations. We show that the data we collected are in good agreement with the presence of two anisotropically specular east and west core hemispheres. The detected travel-time anomalies can only be disclosed by a lattice-preferred orientation of a body-centered-cubic iron aggregate, having a fraction of their [111] crystal axes parallel to the Earth’s rotation axis. This is compelling evidence for the presence of a body-centered-cubic Fe phase at the top of the Earth’s inner core.},
	pages = {9507--9512},
	number = {21},
	journaltitle = {Proceedings of the National Academy of Sciences},
	shortjournal = {{PNAS}},
	author = {Mattesini, Maurizio and Belonoshko, Anatoly B. and Buforn, Elisa and Ramírez, María and Simak, Sergei I. and Udías, Agustín and Mao, Ho-Kwang and Ahuja, Rajeev},
	urldate = {2020-12-22},
	date = {2010-05-25},
	langid = {english},
	pmid = {20457937},
	note = {Publisher: National Academy of Sciences
Section: Physical Sciences},
	keywords = {elastic anisotropy, Fe-bcc/-hcp, molecular dynamics, {PKiKP}/{PKIKP} waves},
	file = {Mattesini et al_2010_Hemispherical anisotropic patterns of the Earth’s inner core.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2010/Mattesini et al_2010_Hemispherical anisotropic patterns of the Earth’s inner core.pdf:application/pdf;Snapshot:/Users/abc123/Zotero/storage/B6RAJJPC/9507.html:text/html}
}

@article{tkalcic_complex_2015,
