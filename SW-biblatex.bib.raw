	file = {Lin_Tsai_2013_Seismic interferometry with antipodal station pairs.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2013/Lin_Tsai_2013_Seismic interferometry with antipodal station pairs.pdf:application/pdf}
}

