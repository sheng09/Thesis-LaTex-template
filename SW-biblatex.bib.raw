@article{tkalcic_shear_2018,
	title = {Shear properties of Earth’s inner core constrained by a detection of J waves in global correlation wavefield},
	volume = {362},
	rights = {Copyright © 2018 The Authors, some rights reserved; exclusive licensee American Association for the Advancement of Science. No claim to original U.S. Government Works. http://www.sciencemag.org/about/science-licenses-journal-article-{reuseThis} is an article distributed under the terms of the Science Journals Default License.},
