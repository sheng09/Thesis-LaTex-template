	note = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/97RG01285},
	file = {Song_1997_Anisotropy of the Earth's inner core.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/1997/Song_1997_Anisotropy of the Earth's inner core.pdf:application/pdf}
}

