	urldate = {2021-01-08},
	date = {2007-06-15},
	langid = {english},
	pmid = {17569860},
	note = {Publisher: American Association for the Advancement of Science
Section: Report},
	file = {Belonoshko et al_2007_Origin of the Low Rigidity of the Earth's Inner Core.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2007/Belonoshko et al_2007_Origin of the Low Rigidity of the Earth's Inner Core.pdf:application/pdf}
}

