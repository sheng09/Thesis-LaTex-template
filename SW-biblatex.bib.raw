@article{yang_characteristics_2008,
	title = {Characteristics of ambient seismic noise as a source for surface wave tomography},
	volume = {9},
	issn = {1525-2027},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1029/2007GC001814},
	doi = {10.1029/2007GC001814},
	abstract = {Interstation cross correlations of ambient seismic noise from 1 year of continuous data at periods between 6 and 50 s are used to study the origin of the ambient noise using stations located in Europe, southern Africa, Asia, and three regions within North America. The signal-to-noise ratios ({SNR}) of Rayleigh waves for positive and negative correlation time lags at periods of 8, 14, 25 and 50 s are used to determine the azimuthal distribution of strong ambient noise sources. Ambient noise in both the primary (10–20 s) and secondary microseism bands (5–10 s) comes dominantly from the directions of relatively nearby coastlines with stronger noise occurring in the Northern Hemisphere in northern winter and in the Southern Hemisphere in southern winter, consistent with the hypothesis that oceanic microseisms are generating this noise. The observed differences in the directivity of noise in the primary and secondary microseism bands are the consequence of propagation and attenuation, rather than the location of generation. At intermediate and long periods ({\textgreater}20 s), there is much less seasonal variation in both signal strength and directivity. We argue that our results are explained most simply by near-coastal sources rather than deep ocean sources at all periods. Although the dominant ambient noise sources are distributed inhomogeneously in azimuth, strong ambient noise emerges from most directions when using recordings that are 1 year in duration. Simulations illustrate that this is what ensures the accuracy of the empirical Green's functions and ambient noise tomography.},
	number = {2},
	journaltitle = {Geochemistry, Geophysics, Geosystems},
	author = {Yang, Yingjie and Ritzwoller, Michael H.},
	urldate = {2021-11-02},
	date = {2008},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2007GC001814},
	keywords = {tomography, ambient seismic noise, cross correlations, Green's functions},
	file = {Yang_Ritzwoller_2008_Characteristics of ambient seismic noise as a source for surface wave tomography.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2008/Yang_Ritzwoller_2008_Characteristics of ambient seismic noise as a source for surface wave tomography.pdf:application/pdf}
}

