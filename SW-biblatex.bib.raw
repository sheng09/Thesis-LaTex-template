@article{mao_phase_2006,
	title = {Phase relations of Fe–Ni alloys at high pressure and temperature},
	volume = {155},
	issn = {0031-9201},
	url = {https://www.sciencedirect.com/science/article/pii/S0031920105002487},
	doi = {10.1016/j.pepi.2005.11.002},
	abstract = {Using a diamond anvil cell and double-sided laser-heating coupled with synchrotron X-ray diffraction, we determined phase relations for three compositions of Fe-rich {FeNi} alloys in situ at high pressure and high temperature. We studied Fe with 5, 15, and 20wt.\% Ni to 55, 62, and 72GPa, respectively, at temperatures up to ∼3000K. Ni stabilizes the face-centered cubic phase to lower temperatures and higher pressure, and this effect increases with increasing pressure. Extrapolation of our experimental results for Fe with 15wt.\% Ni suggests that the stable phase at inner core conditions is hexagonal close packed, although if the temperature at the inner core boundary is higher than ∼6400K, a two phase outer region may also exist. Comparison to previous laser-heated diamond anvil cell studies demonstrates the importance of kinetics even at high temperatures.},
	pages = {146--151},
	number = {1},
	journaltitle = {Physics of the Earth and Planetary Interiors},
	shortjournal = {Physics of the Earth and Planetary Interiors},
	author = {Mao, Wendy L. and Campbell, Andrew J. and Heinz, Dion L. and Shen, Guoyin},
	urldate = {2021-07-15},
	date = {2006-04-14},
	langid = {english},
	keywords = {High pressure, X-ray diffraction, Earth's core, Diamond anvil cell, Iron–nickel alloys, Laser-heating}
}

@article{singh_presence_2000,
	title = {On the Presence of Liquid in Earth's Inner Core},
	volume = {287},
	issn = {0036-8075, 1095-9203},
	url = {https://science.sciencemag.org/content/287/5462/2471},
	doi = {10.1126/science.287.5462.2471},
	abstract = {Seismological studies indicate that the inner core of Earth is anisotropic for compressional waves (P waves), and has low shear wave (S wave) velocity, and high seismic attenuation. Using an effective medium theory for composite materials, we show that the presence of a volume fraction of 3 to 10\% liquid in the form of oblate spheroidal inclusions aligned in the equatorial plane between iron crystals is sufficient to explain the aforementioned seismic phenomena. Variation of S-wave velocity between the polar axis and equatorial plane is more sensitive to the addition of liquid than that of P waves. The liquid could arise from the presence of dendrites or a mixture of elements other than iron that exist in liquid form under inner-core conditions.},
	pages = {2471--2474},
	number = {5462},
	journaltitle = {Science},
	author = {Singh, S. C. and Taylor, M. a. J. and Montagner, J. P.},
	urldate = {2021-07-15},
