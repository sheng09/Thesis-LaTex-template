	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2018GL077338},
	keywords = {seismology, inverse theory, wave propagation, tomography, computational geophysics, Earth structure},
	file = {Fichtner et al_2018_The Collaborative Seismic Earth Model.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2018/Fichtner et al_2018_The Collaborative Seismic Earth Model.pdf:application/pdf}
}

