@article{wang_shear-wave_2021,
	title = {Shear-wave anisotropy in the Earth's inner core},
	volume = {48},
	issn = {1944-8007},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1029/2021GL094784},
	doi = {10.1029/2021GL094784},
	abstract = {Earth's inner core ({IC}) anisotropy is widely used to infer the deep Earth's evolution and present dynamics. Many compressional-wave anisotropy models have been proposed based on seismological observations. In contrast, inner-core shear-wave (J-wave) anisotropy—on a par with the compressional-wave anisotropy—has been elusive. Here, we present a new class of the J-wave anisotropy observations utilizing earthquake coda-correlation wavefield. We establish that the coda-correlation feature I2-J, sensitive to J-wave speed, exhibits time and amplitude changes when sampling the {IC} differently. J-waves traversing the {IC} near its center travel faster for the oblique than equatorial angles relative to the Earth's rotation axis by at least ∼5 s. The simplest explanation is the J-wave cylindrical anisotropy with a minimum strength of ∼0.8\%, formed through the lattice-preferred-orientation mechanism of iron. Although we cannot uniquely determine its stable iron phase, the new observations rule out one of the body-centered-cubic iron models.},
	pages = {e2021GL094784},
	number = {19},
	journaltitle = {Geophysical Research Letters},
	author = {Wang, Sheng and Tkalčić, Hrvoje},
	urldate = {2021-10-06},
	date = {2021},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2021GL094784},
	keywords = {Earth's inner core, iron crystal structure, shear-wave anisotropy},
	file = {Wang_Tkalčić_2021_Shear-Wave Anisotropy in the Earth's Inner Core.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2021/Wang_Tkalčić_2021_Shear-Wave Anisotropy in the Earth's Inner Core.pdf:application/pdf}
}

@article{lin_extracting_2013,
	title = {Extracting seismic core phases with array interferometry},
	volume = {40},
	issn = {1944-8007},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/grl.50237},
	doi = {10.1002/grl.50237},
	abstract = {{AbstractSeismic} body waves that sample Earth's core are indispensable for studying the most remote regions of the planet. Traditional core phase studies rely on well-defined earthquake signals, which are spatially and temporally limited. We show that, by stacking ambient-noise cross-correlations between {USArray} seismometers, body wave phases reflected off the outer core ({ScS}), and twice refracted through the inner core ({PKIKP}2) can be clearly extracted. Temporal correlation between the amplitude of these core phases and global seismicity suggests that the signals originate from distant earthquakes and emerge due to array interferometry. Similar results from a seismic array in New Zealand demonstrate that our approach is applicable in other regions and with fewer station pairs. Extraction of core phases by interferometry can significantly improve the spatial sampling of the deep Earth because the technique can be applied anywhere broadband seismic arrays exist.},
	pages = {1049--1053},
	number = {6},
	journaltitle = {Geophysical Research Letters},
	author = {Lin, Fan-Chi and Tsai, Victor C. and Schmandt, Brandon and Duputel, Zacharie and Zhan, Zhongwen},
