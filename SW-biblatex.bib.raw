	keywords = {body wave, seismic interferometry, seismology, coda, equipartition, scattering},
	file = {Sens-Schönfelder et al_2015_The lack of equipartitioning in global body wave coda.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/2015/Sens-Schönfelder et al_2015_The lack of equipartitioning in global body wave coda.pdf:application/pdf}
