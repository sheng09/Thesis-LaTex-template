	note = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/{GL}013i013p01549},
	file = {Woodhouse et al. - 1986 - Evidence for inner core anisotropy from free oscil.pdf:/Users/abc123/OneDrive - Australian National University/Zotera/1986/Woodhouse et al. - 1986 - Evidence for inner core anisotropy from free oscil.pdf:application/pdf}
